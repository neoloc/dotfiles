! █▓▒░ scorcerer colors
*.foreground:   #c2c2b0
*.background:   #222222
*.cursorColor:  #c2c2b0
! black
*.color0:       #111111
*.color8:       #181818
! red
*.color1:       #aa4450
*.color9:       #ff6a6a
! green
*.color2:       #719611
*.color10:      #b1d631
! yellow
*.color3:       #ff9800
*.color11:      #87875f
! blue
*.color4:       #6688aa
*.color12:      #90b0d1
! magenta
*.color5:       #8f6f8f
*.color13:      #8181a6
! cyan
*.color6:       #528b8b
*.color14:      #87ceeb
! white
*.color7:       #d3d3d3
*.color15:      #c1cdc1
! stylized
*.colorIT:      #719611
*.colorBD:      #d3d3d3
*.colorUL:      #528b8b

URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.scrollBar_floating: false
URxvt.scrollstyle: rxvt
! dont scroll back on output
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true

URxvt.depth:                      32
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxvt*cursorBlink:                true
URxvt*saveLines:                  50000
URxvt.geometry:                   400x400

! -- Handle URLs in console -- !
URxvt.url-launcher:   /usr/bin/firefox
URxvt.underlineURLs:  True
URxvt.matcher.button: 1 

! -- Resize fonts live -- !
URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal
URxvt.keysym.C-equal:  font-size:reset
URxvt.keysym.C-slash:  font-size:show

! -- Jump-Words CTRL+Arrow -- !
URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C

! -- Use CTRL+SHIFT+C/V for Copy/Paste -- !
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:
! URxvt.clipboard.autocopy: true

!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

! -- Fonts -- !
! URxvt.font:xft:Monospace:pixelsize=12
! URxvt.boldfont:xft:Monospace-Bold:pixelsize=12
! URxvt.font:9x15,xft:TerminessTTFNerdFontMono
! URxvt.font: xft:DejavuSansMono:pixelsize=14
URxvt.font: xft:SFMono Nerd Font:size=10

! -- urxvt colours -- !
URxvt.foreground:    #ABB2BF
URxvt.background:    #1E2127
URxvt.cursorColor:   #5C6370
URxvt.highlightColor:#3A3F4B

URxvt.color0:        #1E2127
URxvt.color8:        #5C6370
URxvt.color1:        #E06C75
URxvt.color9:        #E06C75
URxvt.color2:        #98C379
URxvt.color10:       #98C379
URxvt.color3:        #D19A66
URxvt.color11:       #D19A66
URxvt.color4:        #61AFEF
URxvt.color12:       #61AFEF
URxvt.color5:        #C678DD
URxvt.color13:       #C678DD
URxvt.color6:        #56B6C2
URxvt.color14:       #56B6C2
URxvt.color7:        #ABB2BF
URxvt.color15:       #FFFFFF

! -- Fix Blues -- !
*color12: #5d93f0
*color4:  #437fcc

! -- Fix Home End keys -- !
URxvt.keysym.Home: \033[1~
URxvt.keysym.End: \033[4~
URxvt.keysym.KP_Home: \033[1~
URxvt.keysym.KP_End:  \033[4~
